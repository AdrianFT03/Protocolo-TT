        android:layout_marginLeft="168dp"
        android:layout_marginTop="40dp"
        android:onClick="InfoNot"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageButton24"
